    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {
            "textMateRules" : [
                {
                    "scope":"variable.parameter",
                    "settings": {
                        "foreground": "#eb7a74c5",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope" : "variable",
                    "settings": {
                        "foreground": "#00ffff"
                    }
                },
                {
                    "scope" : "entity.name.function",
                    "settings": {
                        "foreground": "#ffc552",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope" : "keyword.operator",
                    "settings": {
                        "foreground": "#cc8500",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope":"constant",
                    "settings": {
                        // "foreground": "#4FC1FF",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope" : "meta.class",
                    "settings": {
                        "fontStyle": "bold"
                    }
                },  
                {
                    "scope" : "string",
                    "settings": {
                        "foreground": "#cfb59c"
                    }
                }
            ]
        },
    },
    "editor.fontSize": 12,
    "python.analysis.diagnosticMode": "workspace",
