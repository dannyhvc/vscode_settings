          }
        },
        {
          "scope": "constant",
          "settings": {
            // "foreground": "#4FC1FF",
            "fontStyle": "bold"
          }
        },
        {
          "scope": "meta.class",
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": "string",
          "settings": {
            "foreground": "#cfb59c"
          }
        },
        {
          "scope": "string.quoted.docstring.multi.python",
